    "keywords": [
        "reactjs",
        "mobx"
    ],
    "author": "rking",
    "license": "ISC",
    "dependencies": {
        "history": "^4.7.2",
        "mobx": "^3.3.1",
        "mobx-react": "^4.3.4",
        "mobx-react-router": "^4.0.1",
        "react": "^16.0.0",
        "react-dom": "^16.0.0",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.2"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/history": "^4.6.1",
        "@types/jest": "^21.1.5",
        "@types/react": "^16.0.21",
        "@types/react-dom": "^16.0.2",
        "@types/react-router": "^4.0.16",
        "@types/react-router-dom": "^4.2.0",
        "awesome-typescript-loader": "^3.3.0",
        "css-loader": "^0.28.7",
