{
    "name": "gen-reacts-app",
    "version": "2.0.0",
    "description": "A cli tool to generate react typescript application",
    "main": "index.js",
    "preferGlobal": true,
    "bin": {
        "gen-reacts-app": "./lib/index.js"
    },
    "scripts": {
        "prepare": "yarn compile",
        "test": "echo \"Error: no test specified\" && exit 1",
        "compile": "tsc --p ./tsconfig.json && cp -r base lib",
        "dev": "npm run compile && gen-reacts-app"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/roger-king/react-typescript-gen.git"
    },
    "keywords": [
        "webpack",
        "reactjs",
        "typescript",
        "cli",
        "plop"
    ],
    "author": "Roger King <rogr.king@gmail.com>",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/roger-king/react-typescript-gen/issues"
    },
    "homepage": "https://github.com/roger-king/react-typescript-gen#readme",
    "dependencies": {
        "chalk": "^2.4.1",
        "commander": "^2.19.0",
        "fs": "^0.0.1-security",
        "inquirer": "^6.2.1"
    },
    "devDependencies": {
        "@types/chalk": "^2.2.0",
        "@types/commander": "^2.12.2",
        "@types/inquirer": "^0.0.43",
        "@types/node": "^10.12.11",
        "ts-node": "^7.0.1",
        "typescript": "^3.2.1"
    }
}
