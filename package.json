{
    "name": "react-typescript-gen",
    "version": "1.0.0",
    "description": "A react.js typescript project generator.",
    "main": "index.js",
    "scripts": {
        "start:dev": "webpack-dev-server --open --config ./build/webpack.dev.conf.js --inline --progress",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "gen": "plop --plopfile build/generators/plopfile.js",
        "gen:init": "plop --plopfile build/generators/plopfile.js init",
        "gen:component": "plop --plopfile build/generators/plopfile.js component",
        "gen:container": "plop --plopfile build/generators/plopfile.js container",
        "gen:service": "plop --plopfile build/generators/plopfile.js service",
        "build": "webpack --config ./build/webpack.prod.conf.js"
    },
    "keywords": [
        "react",
        "reactjs",
        "mobx"
    ],
    "babel": {
        "plugins": [],
        "presets": [
            "react",
            "es2015"
        ]
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "moduleNameMapper": {
            "\\.css$": "identity-obj-proxy",
            "^Components$": "<rootDir>/src/app/components/",
            "^Containers$": "<rootDir>/src/app/containers",
            "^INITAL_STATE$": "<rootDir>/src/app/services/states",
            "^Actions$": "<rootDir>/src/app/services/actions",
            "^Reducers$": "<rootDir>/src/app/reducers"
        },
        "testRegex": "/__test__/.*\\.test.(ts|tsx|js)$",
        "setupFiles": [
            "<rootDir>/__test__/shim.ts",
            "<rootDir>/__test__/enzyme-setup.ts"
        ],
        "coverageDirectory": "./dist/coverage"
    },
    "author": "rking",
    "license": "ISC",
    "dependencies": {
        "history": "^4.7.2",
        "react": "^16.0.0",
        "react-dom": "^16.0.0",
        "react-redux": "^5.0.6",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^4.0.8",
        "redux": "^3.7.2",
        "redux-form": "^7.1.2"
    },
    "devDependencies": {
        "@types/enzyme": "^3.1.5",
        "@types/enzyme-adapter-react-16": "^1.0.1",
        "@types/history": "^4.6.1",
        "@types/jest": "^21.1.5",
        "@types/react": "^16.0.21",
        "@types/react-dom": "^16.0.2",
        "@types/react-redux": "^5.0.12",
        "@types/react-router": "^4.0.16",
        "@types/react-router-dom": "^4.2.0",
        "@types/react-router-redux": "^5.0.10",
        "@types/react-test-renderer": "^16.0.0",
        "@types/redux-devtools": "^3.0.42",
        "@types/redux-devtools-dock-monitor": "^1.1.31",
        "@types/redux-devtools-log-monitor": "^1.0.32",
        "@types/redux-form": "^7.0.9",
        "@types/redux-mock-store": "^0.0.12",
        "autoprefixer": "^7.1.6",
        "awesome-typescript-loader": "^3.3.0",
        "babel-core": "^6.26.0",
        "babel-jest": "^21.2.0",
        "babel-polyfill": "^6.26.0",
        "caniuse-lite": "^1.0.30000769",
        "copy-webpack-plugin": "^4.2.0",
        "css-loader": "^0.28.7",
        "enzyme": "^3.2.0",
        "enzyme-adapter-react-16": "^1.1.0",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.5",
        "friendly-errors-webpack-plugin": "^1.6.1",
        "html-webpack-plugin": "^2.30.1",
        "html-webpack-template": "^6.0.2",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^21.2.1",
        "lost": "^8.2.0",
        "optimize-css-assets-webpack-plugin": "^3.2.0",
        "plop": "^1.9.0",
        "postcss": "^6.0.14",
        "postcss-cssnext": "^3.0.2",
        "postcss-import": "^11.0.0",
        "postcss-loader": "^2.0.8",
        "postcss-mixins": "^6.2.0",
        "postcss-nested": "^2.1.2",
        "react-addons-test-utils": "^15.6.2",
        "react-test-renderer": "^16.1.0",
        "redux-devtools": "^3.4.1",
        "redux-devtools-dock-monitor": "^1.1.2",
        "redux-devtools-log-monitor": "^1.4.0",
        "redux-mock-store": "^1.3.0",
        "redux-thunk": "^2.2.0",
        "source-map-loader": "^0.2.3",
        "style-loader": "^0.19.0",
        "supports-color": "^5.0.0",
        "ts-jest": "^21.2.1",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "url-loader": "^0.6.2",
        "webpack": "^3.8.1",
        "webpack-cleanup-plugin": "^0.5.1",
        "webpack-dev-server": "^2.9.4",
        "webpack-merge": "^4.1.1"
    }
}