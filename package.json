{
    "name": "reactjs-modern-seed",
    "version": "1.0.0",
    "description": "reactjs-modern-seed",
    "main": "index.js",
    "scripts": {
        "start": "cross-env NODE_ENV=development webpack-dev-server --host 0.0.0.0 --port 8001 --history-api-fallback",
        "start:dev": "webpack-dev-server --open --config ./build/webpack.dev.config.js",
        "start:test": "npm-run-all --parallel lint:watch test",
        "test": "karma start --single-run",
        "test:watch": "karma start",
        "gen": "plop",
        "gen:init": "plop init",
        "gen:component": "plop component",
        "gen:container": "plop container",
        "gen:service": "plop service",
        "build:dist": "npm-run-all --parallel lint test dist"
